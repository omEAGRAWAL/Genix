{
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "http://ec2-54-92-139-93.compute-1.amazonaws.com:3000/$1"
      }
    ],
    "redirects": [
      {
        "source": "/old-path",
        "destination": "/new-path",
        "permanent": true
      }
    ],
    "headers": [
      {
        "source": "/api/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ]
  }
  